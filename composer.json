{
    "name": "yuriwf/mautic-api-laravel",
    "description": "Wrapper on the Mautic API for Laravel 6.x",
    "keywords": [
        "laravel",
        "mautic",
        "api",
		"automation",
        "email",
        "marketing"		
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Nurkov",
            "email": "evgeni.nurkov@gmail.com"
        }
    ],
    "require": {
        "php": ">= 5.6",
	"laravel/framework": "^5.7|^6",  
        "mautic/api-library": "2.15.0"
    },
    "autoload": {
        "psr-4": {
            "Gentor\\Mautic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gentor\\Mautic\\MauticServiceProvider"
            ],
            "aliases": {
                "Mautic": "Gentor\\Mautic\\Facades\\Mautic"
            }
        }
    },
    "minimum-stability": "dev"
}
